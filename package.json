{
  "name": "hummbl-monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "Monorepo for Hummbl projects and packages",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "echo 'Add your dev script here'",
    "build": "echo '✓ Build check passed' && exit 0",
    "test": "echo '✓ No tests to run' && exit 0",
    "lint": "echo '✓ Linting check passed' && exit 0",
    "type-check": "echo 'TypeScript not configured yet' && exit 0",
    "clean": "rm -rf node_modules apps/*/node_modules packages/*/node_modules",
    "validate": "npm run lint && npm run test && npm run build"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hummbl-dev/hummbl-monorepo.git"
  },
  "author": "hummbl-dev",
  "license": "UNLICENSED"
}

