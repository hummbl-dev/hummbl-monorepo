{
  "name": "hummbl-io",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml,css,scss}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix --no-error-on-unmatched-pattern",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "test": "NODE_OPTIONS='--max-old-space-size=4096 --expose-gc' vitest run --no-watch --silent --run",
    "test:memory-optimized": "NODE_OPTIONS='--max-old-space-size=2048 --expose-gc' vitest run --no-watch --silent --run --pool=forks --poolOptions.forks.singleFork",
    "test:memory-profile": "NODE_OPTIONS='--max-old-space-size=4096 --expose-gc --inspect-brk' vitest run --no-watch --silent --run",
    "test:analyze-memory": "node --expose-gc scripts/analyzeTestMemory.js",
    "test:optimize": "node scripts/optimizeTestMemory.js",
    "test:analytics": "NODE_OPTIONS='--max-old-space-size=2048 --expose-gc' vitest run src/utils/__tests__/analytics.test.ts --no-watch --silent --run",
    "test:analytics:edge-cases": "NODE_OPTIONS='--max-old-space-size=2048 --expose-gc' vitest run src/utils/__tests__/analytics.edge-cases.test.ts --no-watch --silent --run",
    "test:dev": "NODE_OPTIONS='--max-old-space-size=4096 --expose-gc' vitest",
    "test:ui": "NODE_OPTIONS='--max-old-space-size=4096 --expose-gc' vitest --ui",
    "test:coverage": "NODE_OPTIONS='--max-old-space-size=4096 --expose-gc' vitest run --coverage --run",
    "test:perf": "NODE_OPTIONS='--max-old-space-size=4096 --expose-gc' tsx scripts/testWatcherLoad.ts",
    "test:auth": "NODE_OPTIONS='--max-old-space-size=4096 --expose-gc' tsx scripts/testAuthPerf.ts",
    "test:ci": "NODE_OPTIONS='--max-old-space-size=4096 --expose-gc' pnpm test:coverage && pnpm test:perf",
    "test:ci:auth": "NODE_OPTIONS='--max-old-space-size=4096 --expose-gc' pnpm test:coverage && pnpm test:auth",
    "test:ci:fast": "NODE_OPTIONS='--max-old-space-size=2048 --expose-gc' vitest run --passWithNoTests --silent --run",
    "test:e2e": "NODE_OPTIONS='--max-old-space-size=8192' jest src/__tests__/app.e2e.test.tsx --config=jest.config.js --runInBand --detectOpenHandles --verbose",
    "build:types": "tsc -p cascade/types/tsconfig.json",
    "prebuild": "pnpm run build:types",
    "deploy:staging": "sh ./scripts/deploy-cvp-staging.sh",
    "benchmark:cvp": "tsx scripts/benchmark-cvp.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "phase2:complete": "node ./scripts/phase2-complete.mjs",
    "validate:models": "node ./scripts/validate-models.mjs",
    "audit": "node ./scripts/audit.mjs",
    "telemetry": "node ./scripts/telemetry.mjs",
    "clean": "rimraf node_modules dist build coverage .cache .temp .tmp",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@sentry/browser": "^10.20.0",
    "@sentry/react": "^10.20.0",
    "@sentry/tracing": "^7.120.4",
    "@sentry/types": "^10.20.0",
    "@sentry/vite-plugin": "^4.4.0",
    "@supabase/supabase-js": "^2.75.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.3.3",
    "vite": "^7.1.10"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@eslint/js": "^8.57.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/csv-stringify": "^1.4.3",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.10.9",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "ajv": "^8.17.1",
    "axe-core": "^4.9.1",
    "chokidar": "^5.0.0",
    "chrome-launcher": "^1.1.1",
    "commander": "^14.0.2",
    "csv-stringify": "^6.6.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "globals": "^15.15.0",
    "happy-dom": "^20.0.7",
    "husky": "^9.1.7",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.0.0",
    "lighthouse": "^12.8.2",
    "lint-staged": "^16.2.4",
    "prettier": "^3.1.1",
    "puppeteer": "^24.25.0",
    "rimraf": "^6.0.1",
    "table": "^6.9.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "vitest": "^3.2.4"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(ts|tsx)$": [
        "babel-jest"
      ]
    },
    "testMatch": [
      "**/__tests__/**/*.test.(ts|tsx|js|jsx)",
      "**/?(*.)+(spec|test).(ts|tsx|js|jsx)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "engines": {
    "node": "20.x"
  }
}
